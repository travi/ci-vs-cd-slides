{
  "name": "ci-vs-cd-slides",
  "description": "Spectacle slide deck for talk about the conflict between CI and CD",
  "license": "MIT",
  "private": true,
  "repository": "travi/ci-vs-cd-slides",
  "bugs": "https://github.com/travi/ci-vs-cd-slides/issues",
  "homepage": "https://github.com/travi/ci-vs-cd-slides#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "prebuild": "run-s clean",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "start": "webpack-dev-server",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:integration": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "predeploy": "run-s build",
    "deploy": "netlify deploy --site=$NETLIFY_SITE_ID --auth=$NETLIFY_ACCESS_TOKEN --dir=./lib/ --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/register": "7.4.4",
    "@travi/babel-preset": "3.0.0",
    "@travi/eslint-config-travi": "1.8.1",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "ban-sensitive-files": "1.9.2",
    "chai": "4.2.0",
    "commitlint-config-travi": "1.3.0",
    "cucumber": "5.1.0",
    "cz-conventional-changelog": "2.1.0",
    "file-loader": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.5.0",
    "img-loader": "3.0.1",
    "mustache-loader": "1.4.3",
    "netlify-cli": "2.11.23",
    "npm-run-all": "4.1.5",
    "raw-loader": "3.0.0",
    "remark-cli": "6.0.1",
    "remark-preset-lint-travi": "1.2.0",
    "style-loader": "0.23.1",
    "travis-lint": "1.0.0",
    "url-loader": "2.0.1",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "3.7.2"
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.11.1",
    "redbox-react": "1.6.0",
    "spectacle": "5.5.0"
  }
}
