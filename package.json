{
  "name": "ci-vs-cd-slides",
  "description": "Spectacle slide deck for talk about the conflict between CI and CD",
  "license": "MIT",
  "private": true,
  "repository": "travi/ci-vs-cd-slides",
  "bugs": "https://github.com/travi/ci-vs-cd-slides/issues",
  "homepage": "https://github.com/travi/ci-vs-cd-slides#readme",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "prebuild": "run-s clean",
    "build": "webpack --env production",
    "build:dev": "webpack --env development",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:sensitive": "ban",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:integration": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:smoke": "start-server-and-test 'npm start' http://localhost:5000 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "prestart": "run-s build",
    "start": "serve lib/",
    "dev": "webpack-dev-server",
    "predeploy:netlify": "run-s build",
    "deploy:netlify": "netlify deploy --dir=./lib/ --prod"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/register": "7.8.3",
    "@travi/babel-preset": "3.0.11",
    "@travi/eslint-config": "1.0.31",
    "@travi/eslint-config-cypress": "1.0.11",
    "@travi/eslint-config-react": "1.0.21",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "ban-sensitive-files": "1.9.2",
    "chai": "4.2.0",
    "commitlint-config-travi": "1.3.5",
    "cucumber": "6.0.5",
    "cypress": "3.8.1",
    "cz-conventional-changelog": "3.0.2",
    "file-loader": "4.3.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.0.10",
    "img-loader": "3.0.1",
    "mustache-loader": "1.4.3",
    "netlify-cli": "2.29.0",
    "npm-run-all": "4.1.5",
    "raw-loader": "4.0.0",
    "remark-cli": "7.0.1",
    "remark-preset-lint-travi": "1.3.0",
    "serve": "11.2.0",
    "start-server-and-test": "1.10.6",
    "style-loader": "1.1.2",
    "travis-lint": "1.0.0",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "3.10.1"
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.18",
    "redbox-react": "1.6.0",
    "spectacle": "5.7.1"
  }
}
